<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import javafx.scene.input.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="440.0" prefWidth="530.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fi.ni.gui.fx.IFC2RDFController">
  <children>
    <MenuBar fx:id="myMenuBar" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
                  <Menu mnemonicParsing="false" text="Express to OWL">
                    <items>
                      <MenuItem fx:id="openLDAC2015Version" mnemonicParsing="false" onAction="#openLDAC2015Version" text="Open as LDAC2015">
                           <accelerator>
                              <KeyCodeCombination alt="UP" code="L" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                           </accelerator></MenuItem>
                        <MenuItem disable="true" mnemonicParsing="false" text="Open as AUTCON2015">
                           <accelerator>
                              <KeyCodeCombination alt="UP" code="A" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                           </accelerator></MenuItem>
                        <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem fx:id="saveIfcOWLMenuItem" disable="true" mnemonicParsing="false" onAction="#saveIfcOWL" text="Save ifcOWL">
                           <accelerator>
                              <KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                           </accelerator></MenuItem>
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Convert an IFC file to RDF">
                     <items>
                        <MenuItem fx:id="openLDAC2015Version1" mnemonicParsing="false" onAction="#selectIFCFile" text="Select an IFC file">
                           <accelerator>
                              <KeyCodeCombination alt="UP" code="I" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                           </accelerator>
                        </MenuItem>
                        <MenuItem mnemonicParsing="false" onAction="#selectTargetFile" text="Select the target file">
                           <accelerator>
                              <KeyCodeCombination alt="UP" code="T" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                           </accelerator>
                        </MenuItem>
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem fx:id="convert2RDFMenuItem" disable="true" mnemonicParsing="false" onAction="#convertIFCToRDF" text="Convert to RDF">
                           <accelerator>
                              <KeyCodeCombination alt="UP" code="R" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                           </accelerator>
                        </MenuItem>
                     </items>
                  </Menu>
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" onAction="#closeApplicationAction" text="Quit">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="Q" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#aboutAction" text="About IFC-to-RDF GUI" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="375.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
         <children>
            <Rectangle id="handleOntologiesTxt" arcHeight="5.0" arcWidth="5.0" height="415.0" stroke="BLACK" strokeType="INSIDE" width="530.0">
               <fill>
                  <LinearGradient endX="1.0" endY="1.0" startX="0.009523809523809525">
                     <stops>
                        <Stop color="#93c2ee" />
                        <Stop color="#93c2ee" offset="0.007662835249042145" />
                        <Stop color="#263607" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </fill>
            </Rectangle>
            <Rectangle fx:id="handleLDAC_OnArea" arcHeight="5.0" arcWidth="5.0" height="113.0" layoutX="14.0" layoutY="14.0" stroke="BLACK" strokeType="INSIDE" width="502.0">
               <fill>
                  <LinearGradient endX="1.0" endY="1.0">
                     <stops>
                        <Stop color="#93c2ee" />
                        <Stop color="WHITE" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </fill>
            </Rectangle>
            <Button fx:id="openLDAC2015VersionButton" accessibleHelp="Open EXP schema and convert to in-memory JENA OWL ontology, using the conversion procedure agreed upon at LDAC2015" defaultButton="true" layoutX="33.0" layoutY="49.0" mnemonicParsing="false" onAction="#openLDAC2015Version" prefHeight="25.0" prefWidth="136.0" text="Open as LDAC2015" />
            <Text layoutX="24.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Express to OWL" />
            <TextArea fx:id="handleOnTxt" editable="false" layoutX="195.0" layoutY="49.0" opacity="0.45" prefHeight="61.0" prefWidth="308.0" />
            <Rectangle fx:id="conversionArea" arcHeight="5.0" arcWidth="5.0" height="191.0" layoutX="14.0" layoutY="142.0" stroke="BLACK" strokeType="INSIDE" width="502.0">
               <fill>
                  <LinearGradient endX="1.0" endY="1.0">
                     <stops>
                        <Stop color="#93c2ee" />
                        <Stop color="WHITE" offset="1.0" />
                     </stops>
                  </LinearGradient>
               </fill>
            </Rectangle>
            <Text layoutX="24.0" layoutY="166.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Convert an IFC file to RDF" />
            <Button fx:id="selectIFCFileButton" accessibleHelp="Open EXP schema and convert to in-memory JENA OWL ontology, using the conversion procedure agreed upon at LDAC2015" defaultButton="true" layoutX="33.0" layoutY="186.0" mnemonicParsing="false" onAction="#selectIFCFile" prefHeight="25.0" prefWidth="136.0" text="Select an IFC file" />
            <Label fx:id="labelIFCFile" layoutX="194.0" layoutY="190.0" prefHeight="17.0" prefWidth="308.0" />
            <Button fx:id="selectTargetButton" accessibleHelp="Open EXP schema and convert to in-memory JENA OWL ontology, using the conversion procedure agreed upon at LDAC2015" defaultButton="true" layoutX="32.0" layoutY="222.0" mnemonicParsing="false" onAction="#selectTargetFile" prefHeight="25.0" prefWidth="136.0" text="Select the target file" />
            <Label fx:id="labelTargetFile" layoutX="194.0" layoutY="226.0" prefHeight="17.0" prefWidth="308.0" />
            <Button fx:id="convert2RDFButton" accessibleHelp="Open EXP schema and convert to in-memory JENA OWL ontology, using the conversion procedure agreed upon at LDAC2015" disable="true" layoutX="32.0" layoutY="261.0" mnemonicParsing="false" onAction="#convertIFCToRDF" prefHeight="25.0" prefWidth="136.0" text="Convert to RDF">
               <effect>
                  <ColorAdjust hue="-0.26">
                     <input>
                        <Lighting>
                           <bumpInput>
                              <Shadow color="#dd2f2f" />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </input>
                  </ColorAdjust>
               </effect>
            </Button>
            <Button fx:id="saveIfcOWLButton" accessibleHelp="Save ifcOWL ontology in file system (TTL, RDF/XML, N-3, ...)" disable="true" layoutX="33.0" layoutY="85.0" mnemonicParsing="false" onAction="#saveIfcOWL" prefHeight="25.0" prefWidth="136.0" text="Save expressOWL...">
               <effect>
                  <ColorAdjust hue="-0.26">
                     <input>
                        <Lighting>
                           <bumpInput>
                              <Shadow color="#dd2f2f" />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </input>
                  </ColorAdjust>
               </effect>
            </Button>
            <TextArea fx:id="conversionTxt" editable="false" layoutX="195.0" layoutY="261.0" opacity="0.45" prefHeight="61.0" prefWidth="308.0" />
         </children></AnchorPane>
    
    
  </children>
</VBox>
