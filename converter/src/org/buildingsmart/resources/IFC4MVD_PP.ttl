dce:creator 
	rdf:type owl:AnnotationProperty .

dce:description 
	rdf:type owl:AnnotationProperty .

dce:contributor 
	rdf:type owl:AnnotationProperty .

dce:title 
	rdf:type owl:AnnotationProperty .

dce:format 
	rdf:type owl:AnnotationProperty .

dce:identifier 
	rdf:type owl:AnnotationProperty .

dce:language 
	rdf:type owl:AnnotationProperty .

ifc:ENUMERATION
	rdf:type owl:Class .

ifc:SELECT
	rdf:type owl:Class .

ifc:List
	rdf:type owl:Class ;
	rdfs:subClassOf
		[
			rdf:type owl:Restriction ;
			owl:onProperty ifc:isFollowedBy ;
			owl:allValuesFrom ifc:List
		] ,
		[
			rdf:type owl:Restriction ;
			owl:onProperty ifc:hasNext ;
			owl:allValuesFrom ifc:List
		] .

ifc:hasListContent
	rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
	rdfs:label "hasListContent" ;
	rdfs:domain ifc:List .

ifc:hasNext
	rdf:type owl:ObjectProperty, owl:FunctionalProperty ;
	rdfs:label "hasNext" ;
	rdfs:range ifc:List ;
	rdfs:domain ifc:List ;
	rdfs:subPropertyOf ifc:isFollowedBy .

ifc:isFollowedBy
	rdf:type owl:ObjectProperty, owl:TransitiveProperty ;
	rdfs:label "isFollowedBy" ;
	rdfs:range ifc:List ;
	rdfs:domain ifc:List .

ifc:IfcBuilding
	rdf:type owl:Class .

